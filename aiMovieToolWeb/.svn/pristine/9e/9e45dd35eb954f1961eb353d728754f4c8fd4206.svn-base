.customerMenu li button:focus{text-decoration:underline;outline: none;text-decoration: none;}
.customerMenu li.active button{color:#41392f;background-color: #fffcf3;}
.customerMenu li.active{position:relative;}
.customerMenu li.active:after{content:'';display:inline-block;width:9px;height:5px;position:absolute;bottom:-6px;left:50%;margin-left:-4px;}
.customerMenu li img{vertical-align: middle;margin-right: 3px;}
.customerZone {position: relative;width: 100%;height: 47px;}
.customerZone h3 {font-size: 14px;color: #ffffff;float: left;vertical-align: middle;padding-top: 6px;background:url(../images/common/notice_customer.png) 0 11px no-repeat;padding-left: 8px;}
.customerZone h3 a {color: #ffffff;}
.customerZone h3 a:hover {color: #a9b8d2;}
.customerZone .left {float: left;}
.customerZone .center {float: left;}
.customerZone .right {float: right;}

.customerArea {position:absolute;width:100%;height:100%;font-size:14px;letter-spacing:-0.5px;display:table;}
.customerArea .custMzone {display: table-cell;vertical-align: middle;width: 1122px;}
.customerArea .custMzone .mainBox {width: 1123px;height: 594px;margin: 0 auto;}
.customerArea .custMzone .mainBox h1 {margin-bottom: 15px;}
.customerArea .custMzone .mainBox .leftZone {width: 334px;height: 594px;background-color: #4b4b4b;float: left;padding: 40px 30px;}
.customerArea .custMzone .mainBox .leftZone .login {height: 131px;border-bottom: 1px solid #3f3f3f;}
.customerArea .custMzone .mainBox .leftZone .login dt {width: 68px;height: 68px;background:url(../images/common/login_customer.png) 0 center no-repeat;float: left;margin-right: 10px;}
.customerArea .custMzone .mainBox .leftZone .login dd {color: #c3c3c3;font-size: 14px;padding-top: 13px;}
.customerArea .custMzone .mainBox .leftZone .login dd h3 {color: #ffffff;font-size: 19px;margin-bottom: 13px;letter-spacing: 1px;}
.customerArea .custMzone .mainBox .leftZone .login button {color: #242424;font-size: 14px;background-color: #737373;border-radius: 3px;text-align: center;padding: 8px 11px;border: 0px;font-weight: 600;outline: none;}
.customerArea .custMzone .mainBox .leftZone .login button:hover {color: #ffffff;}
.customerArea .custMzone .mainBox .leftZone .menu {margin-top: 25px;}
.customerArea .custMzone .mainBox .leftZone .menu li {border-radius: 5px;background-color: #3b3b3b !important;color: #c3c3c3;height: 50px;text-align: center;margin: 8px 0;font-size: 17px;font-weight: 600;line-height: 50px;background:url(../images/common/icon_customer.png) center right 15px no-repeat;}
.customerArea .custMzone .mainBox .leftZone .menu li a {color: #c3c3c3;display: block;height: 50px;}
.customerArea .custMzone .mainBox .leftZone .menu li a:hover {color: #949494;}
.customerArea .custMzone .mainBox .rightZone {width: 789px;height: 594px;background-color: #ffffff;border-top: 1px solid #dbdfe3;border-right: 1px solid #dbdfe3;border-bottom: 1px solid #dbdfe3;float: right;}
.customerArea .custMzone .mainBox .rightZone .imgZone {width: 788px;height: 422px;background:url(../images/common/img_customer.png) top left no-repeat;border-bottom: 1px solid #dbdfe3;position: relative;}
.customerArea .custMzone .mainBox .rightZone .noticeZone {width: 270px;position: absolute;top: 120px;right: 30px;}
.customerArea .custMzone .mainBox .rightZone .noticeZone h1 {color: #ffffff;font-size: 24px;margin-bottom: 15px;display: inline-block;}
.customerArea .custMzone .mainBox .rightZone .noticeZone li {color: #ffffff;font-size: 15px;margin: 13px 0;padding-left: 10px;background:url(../images/common/notice_customer.png) center left no-repeat;}
.customerArea .custMzone .mainBox .rightZone .noticeZone li a {color: #ffffff;display: block;text-overflow:ellipsis;overflow: hidden;white-space: nowrap;}
.customerArea .custMzone .mainBox .rightZone .noticeZone li:hover {color: #ffffff;text-decoration: underline;}
.customerArea .custMzone .mainBox .rightZone .noticeZone .on {color: #fff8be;background:url(../images/common/notice_customer_on.png) center left no-repeat;}
.customerArea .custMzone .mainBox .rightZone .noticeZone .on a {color: #fff8be;}
.customerArea .custMzone .mainBox .rightZone .noticeZone span {position: absolute;top: -4px;right: 0px;}
.customerArea .custMzone .mainBox .rightZone .callZone li {width: 262px;height: 102px;float: left;background:url(../images/common/line_customer.png) top left no-repeat;margin-top: 33px;}
.customerArea .custMzone .mainBox .rightZone .callZone dl {width: 195px;margin: 0 auto;padding-top: 15px;}
.customerArea .custMzone .mainBox .rightZone .callZone dl dt {float: left;margin-right: 10px;width: 65px;height: 65px;}
.customerArea .custMzone .mainBox .rightZone .callZone dl dd {text-align: center;color: #646464;font-size: 16px;padding-top: 10px;}
.customerArea .custMzone .mainBox .rightZone .callZone dl dd h2 {color: #000000;font-size: 35px;margin-bottom: 30px;}
.customerArea .custMzone .mainBox .rightZone .callZone .callT {color: #df0024;font-size: 36px;}

.callZoneImg1 {background:url(../images/common/call_customer1.png) top left no-repeat;}
.callZoneImg2 {background:url(../images/common/call_customer2.png) top left no-repeat;}
.callZoneImg3 {background:url(../images/common/call_customer3.png) top left no-repeat;}
