/* search */
.search{z-index:10}
.search table{width:100%;margin-left:-1px;border-bottom:1px solid #dcdcdc;border-left:0;margin-bottom:20px;}
.search table th{padding:8px;background:#f8f8f8;border-left:1px solid #dcdcdc;border-top:1px solid #dcdcdc;color:#333333;text-align:left}
.search table th:first-child{border-left: none;}
.search table tbody tr th.type{border-left:1px solid #dcdcdc}
