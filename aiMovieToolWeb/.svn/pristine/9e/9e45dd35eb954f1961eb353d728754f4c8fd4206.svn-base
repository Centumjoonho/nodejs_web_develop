@charset "utf-8";
@import url(base.css);

/* layout */
body {height:100%; -webkit-font-smoothing: antialiased; -webkit-text-size-adjust: 100%;background-color:#f4f6f8;}
#wrap{position:relative}
#wrap_custumer{position:relative}
#header{width:100%}

#header h1{width:203px;height:46px; margin: 7px 0;margin-left: 20px;}
/* #header h1 a{text-indent:-9999px;font-size:1px;line-height:0px;color:transparent;width:203px;height:38px;display:block;} */
#header #gnb{width:100%;zoom:1;height: 150px; background-color:#465161; border: 1px solid #465161;}
#header #gnb:after{content:"";display:block;clear:both}
#header #gnb{background:#fff;/*padding: 20px 0;*/}
#header #gnb li{width:25%;background:#fff;color:#465161;text-align:center;float: left;font-size: 16px;font-weight: 600;}
#header #gnb > li > a{font-size: 20px; background-color: #465161; width: 100%; display: block; height:50px; line-height: 50px; color:#fff}
#header .gnb li ul li a{color: #465161;}
#header #gnb li a:hover{color: #ffd800;}
#header #gnb li .on{color: #ffd800;}
#header #gnb li ul{width:100%;display: flex; align-content: flex-start; flex-direction: column;flex-wrap: wrap;}
#header #gnb li ul li{width: 100%; margin-bottom: 8px;}
#header #gnb li ul li:first-child{margin-top: 20px;}
#header #gnb li ul li a:hover{color: #ff0000;}
#header #gnb li ul li .on{color: #ff0000;}

#header .utitily{position:absolute;top:13px;right:20px;}
#header .utitily span{color:#686868;background:url(../images/common/utitiy_login.png) left no-repeat;padding: 6px 0 6px 32px;font-size:13px}
#header .utitily a.btnLogout{display:inline-block;margin-left:12px;font-weight:bold;border-radius:3px;padding:5px 10px 2px 10px;background:#fff;border:1px solid #ddd;color:#606061;}
#header .utitily a.btnLogout:hover{border:1px solid #6d6e71;color:#6d6e71;}

#contents{border: 1px solid #e8ebee;background-color: #FFFFFF;padding: 25px;width: 100%;border-radius: 6px;margin-right: 20px;margin-left: 20px;overflow: hidden;min-height: 750px;}

#footer{padding:0 0 20px 0;color:#7b7b7b;text-align:center;margin-top: 20px;font-size: 13px;}

.uploadFileBox {border: 1px dashed #aaa;padding: 0.4%;background-color: #ffffff;display: inline-block;margin-right: 5px;}
.uploadinput {width: 350px;height: 20px !important;}


/***** common ****/
/* input */
input[type="password"]:disabled,
input[type="password"].disabled{background-color: #f1eeee !important;color:#777}

.radio{vertical-align:middle;}
input[type="text"]:disabled,
input[type="text"].disabled{background-color: #f1eeee !important;color:#777}
input.text.txt-right{padding-right:5px;text-align:right}
.file{height:25px;padding:2px 0;border:1px solid #DDDDDD}



/***** login 시작*****/
.loginBox{position:absolute;width:100%;height:100%;display:table;text-align:center;font-size:14px;letter-spacing:-1px;background:url(../images/common/login_bg.jpg) center top no-repeat;-webkit-background-size: cover;-moz-background-size: cover;background-size: cover;-o-background-size: cover;}
.loginBox .login{display:table-cell;vertical-align:middle;}
.loginBox .login > div{width:470px;height:505px;margin:0 auto;text-align:left;}
.loginBox .login .box{margin-top:20px;border:1px solid #d7d7d7;background-color:#fff;padding:0 65px;border-top: 4px solid #df0024;}
.loginBox .login .box .titBox{margin-top:50px;padding-bottom:13px;}
.loginBox .login .box .titBox h1 a{background:url(../images/common/login_logo.gif) 24px center no-repeat;text-indent:-9999px;font-size:1px;line-height:0px;color:transparent;height:52px;display:block;}
.loginBox .login .box .txtBox{padding-bottom: 48px; color:#5b5b5b;}
.loginBox .login .box .txtBox h2{color:#000000;line-height:18px;font-size:25px;font-weight:600;text-align: center; border-bottom: 1px solid #dfdfdf;padding-bottom: 20px;margin: 15px 0 20px 0;}
.loginBox .login .box .txtBox .text{font-size:14px;text-align: center;}
.loginBox .login .box .txtBox1{padding-bottom: 17px; color:#5b5b5b;}
.loginBox .login .box .txtBox1 h2{color:#000000;line-height:18px;font-size:25px;font-weight:600;text-align: center; border-bottom: 1px solid #dfdfdf;padding-bottom: 20px;margin-top: 15px;}
.loginBox .login .box .txtBox1 .text{line-height:17px;font-size:14px;text-align: center;margin-top: 18px;}
.loginBox .textSave {font-size:12px;margin-top:25px;display: inline-block;}
.loginBox .textSave li {float: left;color: #404040;font-size: 14px;margin-right: 10px;letter-spacing: -0.5px;}
.loginBox .textSave li:last-child {margin-right: 0px;}
.loginBox .login .box .txtBox .loginForm{margin-top:25px;}
.loginBox .login .box .txtBox li {float: left;font-size: 15px;}
.loginBox .btn .btnLogin{width:100%;height: 40px;border:1px solid #2a2620;color:#fff;font-size:17px;background: #2a2620;}
.loginBox .btn .btnLoginN{width:100%;height: 40px;border:1px solid #878787;color:#d5d5d5;font-size:17px;background: #878787;}
.loginBox .messegeT  {width: 100%;font-family: 'Nanum Barun Gothic','나눔바른고딕',sans-serif;color: #df0024;font-size: 14px;line-height: 17px;display: inline-block;overflow: hidden !important;}
.loginForm ul{position:relative;}
.loginForm ul li{}
.loginForm .labelInput{position:relative;margin-bottom:7px;width: 100%;}
.loginForm .labelInput label{position:absolute;top:50%;left:12px;margin-top:-7px;color:#333333;font-weight: 600;font-size: 15px;}
.loginForm .labelInput input[type="text"], .loginForm .labelInput input[type="password"]{width:100%;height:38px;line-height:38px;padding:0 5px 0 100px;border:1px solid #c0c0c0;color:#333333;font-size: 15px;font-weight: 600;box-sizing: border-box;}
.loginForm .idSave{font-size:12px;margin-top:15px;}
.loginForm .btn{position: relative;}
.loginForm .captcha {position: relative;border: 1px solid #d6d6d6;background-color: #f9f9f9;padding: 17px 15px;width: 100%;float: left;margin: 5px 0 0 0;}
.loginForm .captcha li {float: left;}
.loginForm .captcha input {border: 1px solid #dfdfdf;background-color: #FFFFFF;height: 33px;padding: 7px;margin-top: 10px;}
.loginForm .infoview {background-color: #eeeeee;text-align: center;font-size: 15px;font-weight: 600;color: #000000;padding: 12px 0 11px 0;margin-bottom: 10px;width: 75%;}
.loginForm .btn_certi{background-color: #7a828d;text-align: center;font-size: 13px;color: #ffffff;padding: 12px 0 11px 0;width: 25%;position: absolute;top: 0px;right: 0px;border: 0px;outline: none;}
.loginForm .btn_certi:hover{background-color: #939da9;color: #ffffff;}
.loginForm .timeT{float: right;font-size: 13px;font-weight: 600;color: #000000;display: inline-block;margin: 10px 0 22px 0;}

.loginForm .txtZone{width: 100%;border-top: 1px solid #34415c;border-left: 1px solid #eeeeee;border-right: 1px solid #eeeeee;border-bottom: 1px solid #eeeeee;background-color: #ffffff;padding: 15px;font-size: 13px;line-height: 20px;letter-spacing: -0.5px;}
.loginForm .txtZone li {background:url(../images/common/title_icon2.gif) 0 7px no-repeat;padding-left: 7px;margin-bottom: 3px;}
.loginForm .txtZone1{width: 100%;border-top: 1px solid #34415c;border-left: 1px solid #eeeeee;border-right: 1px solid #eeeeee;border-bottom: 1px solid #eeeeee;background-color: #ffffff;padding: 15px;font-size: 13px;line-height: 20px;letter-spacing: -0.5px;}
.loginForm .txtZone1 h3 {font-size: 15px;font-weight: 600;line-height: 20px;letter-spacing: -0.5px;margin-bottom: 10px;}
.loginForm .txtZone1 li {margin-bottom: 3px;}

