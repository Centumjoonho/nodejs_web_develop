/***** no page *****/
.noPageBox{width:670px;margin:0 auto;border:1px solid #d3d3d3;padding:35px 40px;border-top:5px solid #465161;background-color: #ffffff;}
.noPageBox .noPage{padding:7px 0 20px 120px;background:url(../images/common/icon_noPage.png) top left no-repeat;border-bottom:1px solid #d3d3d3;letter-spacing: -0.5px;}
.noPageBox p{color:#333;font-family: 'Nanum Barun Gothic','나눔바른고딕',sans-serif;}
.noPageBox .errorNum{color:#e84e3e;}
.noPageBox .bigText{font-size:22px;line-height:31px;font-weight:600;}
.noPageBox .smallText{font-size:16px;line-height:25px;color: #696868;font-size: 15px;}
.noPageBox .bigText + .smallText{margin-top:10px;}
.noPageBox .noPage + .btnBox{margin-top:20px;text-align: center;}
.noPageBox .btn{background-color: #ffffff;border: 1px solid #808080;border-radius: 2px;font-size: 13px;padding: 5px 10px;outline: none;color: #606060;font-weight: 600;}
.noPageBox .btn:hover{border: 1px solid #e75a71;color: #e75a71;}


